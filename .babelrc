{
  "plugins": [
    "react-hot-loader/babel",
    ["@babel/plugin-proposal-decorators", {"legacy": true}],
    "@babel/plugin-proposal-class-properties",
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "Firefox >= 57",
            "Safari >= 10",
            "Chrome >= 60",
            "IE >= 12",
            "Edge >= 14"
          ]
        }
      }
    ],
    "@babel/preset-react"
  ],
  "sourceMaps": true
}
