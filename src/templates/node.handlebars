<span 	class="blocks-node blocks-{{type}}" 
		tabindex="1" role="treeitem" 
		aria-label="{{node.options.aria-label}}"
		aria-describedby="block-node-{{node.options.comment.id}}"
		aria-multiselectable="true"
		aria-selected="false"
		id="block-node-{{node.id}}">{{> @partial-block}}{{{renderNode node.options.comment}}}</span>
